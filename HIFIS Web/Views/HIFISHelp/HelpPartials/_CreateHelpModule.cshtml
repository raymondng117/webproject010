@model HelpModuleModel 
@Html.HiddenFor(model => model.HelpModuleID)
<div class="row">
    <div class="col-sm-6">  
        @Html.Label(Labels.HifisHelp_ModuleCategory) @Html.Partial("Common/_HelpMandatory")
        <br />
        @Html.DropDownListFor(model => model.FriendlyNameID, ViewBag.FriendlyNames as SelectList)
    </div>
    <div class="col-sm-6">  
        @Html.Label(Labels.HifisHelp_ModuleType) @Html.Partial("Common/_HelpMandatory")
        <br />
        @Html.DropDownListFor(model => model.CategoryTypeID, ViewBag.CategoryTypes as SelectList)
    </div>
</div>
<div class="row">
    <div class="col-sm-6">   
        @Html.Label(Labels.HifisHelp_NameE) @Html.Partial("Common/_HelpMandatory")
        <br />
        @Html.TextBoxFor(model => model.NameE)                            
    </div>
    <div class="col-sm-6">   
            @Html.Label(Labels.HifisHelp_NameF) @Html.Partial("Common/_HelpMandatory")
            <br />
        @Html.TextBoxFor(model => model.NameF)                            
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        @Html.Label(Labels.HifisHelp_DescriptionE) @Html.Partial("Common/_HelpMandatory")     
        <br />
        @Html.EditorFor(model => model.ModuleDescriptionE, "TinyMCE_MaterialHelp")
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        @Html.Label(Labels.HifisHelp_DescriptionF) @Html.Partial("Common/_HelpMandatory")     
        <br />
        @Html.EditorFor(model => model.ModuleDescriptionF, "TinyMCE_MaterialHelp")
    </div>
</div>