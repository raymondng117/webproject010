@model ClientVitalsViewModel
@{
    ViewBag.ViewTitle = Labels.CreateAndAddMember;

    ViewBag.LayoutView = LayoutPage.ContentOnly;
}

@Html.Partial("_ValidationSummaryOuter")

<div class="row">

    @using (Html.BeginForm("CreateFamilyMember", "ClientTombstone", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
    {

        <div class="col-md-8">

            @Html.Partial("_ClientVitalsNew", Model)

            <div class="row">
                <div class="col-xs-4"></div>
                <div class="col-xs-8">
                    <div class="form-group">
                        <button type="submit" class="saveButton" name="submitBtn" value="default">@Labels.Save</button>
                        @Html.ActionLink(Labels.Cancel, "ClientFamily", new { id = @Model.FromClientID }, new { @class = "cancelButton", alt=Labels.Cancel, aria_label = Labels.Cancel, @role = "button"  })
                    </div>
                </div>
            </div>
        </div>
    }

</div>


