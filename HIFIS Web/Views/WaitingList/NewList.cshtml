@model WaitingListViewModel
@{
    ViewBag.ViewTitle = Labels.viewTitle_NewWaitingLists;  
    
    ViewBag.LayoutView = LayoutPage.ContentOnly;
}

@Html.Partial("_ValidationSummaryOuter")

<div>
    @using (Html.BeginForm("NewList", "WaitingList", FormMethod.Post, new { @class = "form-horizontal" }))
    {
        @Html.AntiForgeryToken()
        @Html.Partial("_WaitingList");        

        <div class="form-group">
			<div class="col-sm-offset-5 col-sm-7">
				<button type="submit" class="saveButton  ">@Labels.Save</button>
                @Html.ActionLink(Labels.Cancel, "List", "WaitingList", new { @class = "cancelButton", alt = Labels.Cancel, aria_label = Labels.Cancel, @role = "button" })
			</div>
		</div>
    }
</div>