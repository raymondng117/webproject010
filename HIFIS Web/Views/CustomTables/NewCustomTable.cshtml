@model CustomTableViewModel
@{
    ViewBag.ViewTitle = Labels.viewTitle_NewCustomTable;
    
    ViewBag.LayoutView = LayoutPage.ContentOnly;
}

@Html.Partial("_ValidationSummaryOuter")

<div class="row">
    @using (Html.BeginForm("NewCustomTable", "CustomTables", FormMethod.Post, new { @class = "form-horizontal" }))
    {
        <div class="col-sm-12">

            @Html.AntiForgeryToken()
            @Html.Partial("_CustomTable", Model)

            <div class="row">
                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <button type="submit" class="saveButton">@Labels.Save</button>
                        @Html.ActionLink(Labels.Cancel, "CustomTablesList", null, new { @class = "cancelButton", alt=Labels.Cancel, aria_label = Labels.Cancel, @role = "button"  })
                    </div>
                </div>
            </div>
        </div>
    }
</div>