@model List<CustomTableFieldViewModel>

@if (!Model.IsEmptyOrNull())
{
    <div style="height:@(Model.Max(model => model.YCoordinate) - Model.Min(model => model.YCoordinate) + 50)px">
        @for (int i = 0; i < Model.Count; ++i)
        {
            <div style="position:absolute; top:@(Model[i].YCoordinate)px; left:@(Model[i].XCoordinate)px;">
                @Html.Partial("_CustomTableFieldEditor", Model[i], (ViewData["InputNamePrependString"] as string ?? String.Empty) + "[" + i + "].", true)
            </div>
        }
    </div>
}