@model ProgramListViewModel
@{
    ViewBag.ViewTitle = Labels.viewTitle_ClientProgramList;

    ViewBag.LayoutView = LayoutPage.Client;
}
@Html.Partial("_ValidationSummaryOuter")
    
<div class="table-responsive">
    <table class="wb-tables table table-striped table-hover" data-wb-tables='{ "info": false, "order": [[3, "desc"]]}'>
    <thead>
        <tr>
            <th>@Labels.ServiceProvider</th>
            <th>@Labels.ModuleName</th>
            <th>@Labels.ProgramName</th>
            <th>@Labels.Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var clientProgram in Model.ClientPrograms)
        {
            <tr>
                <td>@clientProgram.OwnerOrgName</td>
                <td>@clientProgram.myServiceTypeName</td>
                <td>@clientProgram.Name</td>
                <td>@Html.DisplayFor(model => clientProgram.DateStart) @(clientProgram.DateEnd == null ? " - " : " - " + @Html.DisplayFor(model => clientProgram.DateEnd)) </td>
            </tr>
        }
    </tbody>
</table>
</div>
