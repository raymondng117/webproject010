@model FollowUpPlacementViewModel

<section id="Modal_FollowUpDisplay" class="modal-dialog modal-content overlay-def">
    <header class="modal-header">
        <h2 class="modal-title">@((Model.FollowUpHeader.ServiceTypeID == null) ? "" : WebHelper.GetTextFromCache(CachedTableTypes.HIFIS_ServiceTypes, ViewBag.CurrentCulture, Model.FollowUpHeader.ServiceTypeID) + " " + Labels.FollowUp) </h2>
    </header>
    <div class="modal-body displayView">
        @Html.Partial("~/Views/FollowUps/_FollowUpHeaderDisplay.cshtml", (FollowUpViewModel)Model.FollowUpHeader)
        <div class="row">
            <div class="col-sm-12" style="margin-top: 10px; text-align: center">
                <label class="control-label wdth-strct-200">Clients:</label>
            </div>
        </div>
        @Html.Partial("~/Views/FollowUps/_PlacementFollowUpDetailsDisplay.cshtml", Model)
    </div>

    <div class="modal-footer" style="background-color: white;">
        <button id="" class="btn btn-primary popup-modal-dismiss cancelButton" type="button">@Labels.Close</button>
    </div>
</section>

<script>
    //Get everything to display correctly
    init_hifis();
    autoWidthBoot('.modal-body');
</script>


