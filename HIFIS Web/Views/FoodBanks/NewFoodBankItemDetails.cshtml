@model FoodBankItemDetailViewModel
@{ 
    ViewBag.ViewTitle = Labels.viewTitle_NewFoodBankItem;
    
    ViewBag.LayoutView = LayoutPage.ContentOnly;
}

@Html.Partial("_ValidationSummaryOuter")

<div>
    @using (Html.BeginForm("NewFoodBankItemDetails", "FoodBanks", FormMethod.Post, new { @class = "form-horizontal" }))
    {
        @Html.Partial("_FoodBankItemDetails", Model)
        
        <div class="form-group">
			<div class="col-sm-offset-2 col-sm-10">
				<button type="submit" class="saveButton">@Labels.Save</button>
                 @Html.ActionLink(Labels.Cancel, "FoodBankItemsList", "FoodBanks", new { @class = "cancelButton", alt=Labels.Cancel, aria_label = Labels.Cancel, @role = "button" })
			</div>
		</div>        
    }
</div>  
