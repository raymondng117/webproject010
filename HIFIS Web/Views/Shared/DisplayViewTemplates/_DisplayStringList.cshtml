@model DisplayStringListViewModel

@{ Model.Names = ViewBag.ServiceProgramNames as List<string>; }

<div class="form-group">
    @*<div class="control-label col-sm-2">*@
    <div style="text-align: right;">
        @Html.Raw(Model.Label)
    </div>
    <div id="programsDisplayText" class="col-sm-9" style="margin-top: 8px;">

        @if(Model.Names != null) {
            for (int i = 0; i < Model.Names.Count(); i++)
            {
                if (i != Model.Names.Count() - 1)
                {
                    <text>@Model.Names[i], </text>
                }
                else
                {
                    <text>@Model.Names[i]</text>                        
                }
            }
        }
    </div>
</div>